{
  "name": "docgen",
	"displayName": "DocGen",
	"publisher": "truongthinh",
	"description": "",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.65.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:docgen.replaceWithVaryLength",
		"onCommand:docgen.replaceWithFixedLength",
		"onCommand:docgen.genFunctionDesc",
		"onCommand:docgen.genSingleLineFuncDes",
		"onCommand:docgen.test"
	],
	"main": "./extension.js",
	"contributes": {
		"keybindings": [
            {
                "command": "docgen.replaceWithVaryLength",
                "key": "alt+Z"
            },
			{
                "command": "docgen.replaceWithFixedLength",
                "key": "alt+X"
            },
			{
                "command": "docgen.genFunctionDesc",
                "key": "alt+C"
            },
			{
                "command": "docgen.genSingleLineFuncDes",
                "key": "alt+V"
            },
			{
                "command": "docgen.test",
                "key": "alt+B"
            }
        ]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.65.0",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.0",
        "@types/node": "14.x",
        "eslint": "^8.9.0",
        "glob": "^7.2.0",
        "mocha": "^9.2.1",
        "typescript": "^4.5.5",
        "@vscode/test-electron": "^2.1.2"
    }

}
